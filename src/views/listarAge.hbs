                        <th scope="col">Actualizar</th>
                        <th scope="col">Eliminar</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <th scope="row">{{_id}}</th>
                        <td>{{nombre_cliente}}</td>
                        <td>{{Telefono_cliente}}</td>
                        <td>{{hora_inicio_}}</td>
                        <td><label class="switch">
                          <input type="checkbox">
                          <span class="slider round"></span>
                        </label></td>
                        <td><a href="/cliente/update/{{id_Agenda}}"><button type="button" class="btn btn-success"><i class="bi bi-pencil-square"></i></button></a></td>
                        <td><a href="/cliente/eliminar/{{id_Agenda}}"><button type="button" class="btn btn-danger"><i class="bi bi-trash"></i></button></a></td>
                      </tr>
                 
                    </tbody>
